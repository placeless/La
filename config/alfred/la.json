{
  "providers": {
    "google": {
      "endpoint": "https://generativelanguage.googleapis.com/v1beta/models",
      "api_key": "",
      "models": {
        "gemini-2.5-flash": "gemini-2.5-flash-preview-04-17",
        "gemini-2.0-flash": "gemini-2.0-flash"
      }
    },
    "google_openai": {
      "endpoint": "https://generativelanguage.googleapis.com/v1beta/openai/chat/completions",
      "api_key": "",
      "models": {
        "gemini-2.0-flash-lite": "gemini-2.0-flash-lite-preview-02-05",
        "learnlm-1.5-pro": "learnlm-1.5-pro-experimental"
      }
    },
    "openrouter": {
      "endpoint": "https://openrouter.ai/api/v1/chat/completions",
      "api_key": "",
      "models": {
        "deepseek-chat-v3": "deepseek/deepseek-chat-v3-0324:free",
        "deepseek-r1": "deepseek/deepseek-r1:free",
        "llama-4-maverick": "meta-llama/llama-4-maverick:free"
      }
    },
    "groq": {
      "endpoint": "https://api.groq.com/openai/v1/chat/completions",
      "api_key": "",
      "models": {
        "qwen-qwq-32b": "qwen-qwq-32b"
      }
    }
  },
  "tasks": {
    "explain": {
      "description": "Explain language elements in the input.",
      "provider": "google_openai",
      "model": "gemini-2.0-flash",
      "parameters": {
        "temperature": 0.3
      },
      "prompts": "explain.md"
    },
    "translate": {
      "description": "Translate input to target (Opt key) language, default Chinese.",
      "provider": "google_openai",
      "model": "gemini-2.0-flash-lite",
      "parameters": {
        "temperature": 0
      },
      "prompts": "rewrite.md"
    },
    "ask": {
      "description": "Ask anything.",
      "provider": "google",
      "model": "gemini-2.5-flash",
      "parameters": {
        "temperature": 0.1
      },
      "tools": [
        {
          "googleSearch": {}
        }
      ],
      "max_context": 4,
      "prompts": "You are Gemini, an AI assistant created by Google.",
      "extra": "profile.md"
    }
  }
}
